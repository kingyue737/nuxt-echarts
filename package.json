{
  "name": "nuxt-echarts",
  "version": "1.0.0",
  "description": "Nuxt Module for Apache EChartsâ„¢",
  "keywords": [
    "vue",
    "nuxt",
    "echarts",
    "data-visualization",
    "charts",
    "ssr"
  ],
  "author": "Yue JIN <yuejin13@fudan.edu.cn>",
  "repository": "kingyue737/nuxt-echarts",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./runtime/types": "./dist/runtime/types.js"
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && npm publish --registry=\"https://registry.npmjs.org/\" && git push --follow-tags",
    "release:nightly": "pnpm changelogen --canary --publish --publishTag nightly",
    "docs:dev": "nuxi dev docs",
    "docs:build": "nuxi build docs",
    "docs:types": "nuxi typecheck docs",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && nuxi typecheck playground"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxthub/core": "^0.9.0",
    "@types/node": "^22.17.1",
    "changelogen": "^0.6.2",
    "echarts": "^6.0.0",
    "embla-carousel": "^8.6.0",
    "eslint": "^9.33.0",
    "nuxt": "^4.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vitest": "^3.2.4",
    "vue-echarts": "^8.0.0-beta.1",
    "vue-tsc": "^3.0.5",
    "wrangler": "^4.28.1"
  },
  "peerDependencies": {
    "echarts": "^6.0.0",
    "vue-echarts": "^8.0.0-beta.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "echarts": "6",
        "@nuxt/kit": "4",
        "vue-tsc": "3",
        "vue-component-type-helpers": "3.0.5"
      }
    }
  },
  "stackblitz": {
    "startCommand": "pnpm run dev:prepare && pnpm run dev"
  }
}
